  ISSN                     = {1053-587X},
  Keywords                 = {Markov chain scheme;Monte Carlo methods;approximate filter distributions;approximate smoothing;backward kernels;backward sampling;conditional distribution;forward filtering;latent state trajectory;particle filters;smoothing distribution;state-space models;Markov processes;Monte Carlo methods;approximation theory;particle filtering (numerical methods);smoothing methods;state-space methods;statistical distributions;},
